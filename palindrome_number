class GFG {
	public static void main (String[] args) {
		
		System.out.println(palindrome(12321)); 
		
	}
	
	public static boolean palindrome(int n){
	    
	    int m=n;
	    int rev=0;
	    int rem=0;
	    
	    while(n!=0){
	        
	        rem=n%10;
	        rev=rev*10+rem;
	        n=n/10;
	        
	    }
	    
	    if(rev==m){
	        return true;
	    }else{
	        return false;
	    }
	    
	}
}
