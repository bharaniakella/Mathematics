class GFG {
	public static void main (String[] args) {
		
		System.out.println(factorial(0)); 
		
	}
	
	public static int factorial(int n){
	    if(n==0 | n==1){
	        return 1;
	    }
	    
	    int i=2;
	    int fact=1;
	    while(i<=n){
	        fact=fact*i;
	        i++;
	    }
	    return fact;
	}

}
